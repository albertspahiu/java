import java.util.Scanner;
public class K2C {

	public static void main(String[] args) {
		
		double c = 0.0;
		double k = 0.0;
		
		Scanner hyrje = new Scanner(System.in);
		
		System.out.print("Jepni temperaturen ne Kelvin: ");
		k = hyrje.nextDouble();

		c = k-273.15;

		System.out.print("Temperatura ne Celcius është: ");
		System.out.print(c);
	}
}
